{
  "productName": "plenty",
  "version": "0.0.0",
  "identifier": "studio.darksoil.plenty",
  "build": {
    "beforeBuildCommand": "pnpm -F ui build && pnpm happ:build-debug",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../lobby/dist"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "deep-link": {
      "mobile": [],
      "desktop": {
        "schemes": ["plenty"]
      }
    }
  }
}
